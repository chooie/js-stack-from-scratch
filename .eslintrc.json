{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "compat",
    "flowtype"
  ],
  "rules": {
    "compat/compat": "error",
    "no-console": "off",
    "quotes": ["error", "double", {
      "avoidEscape": true,
      "allowTemplateLiterals": true
    }],
    "semi": "error",
    "strict": "off"
  },
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  }
}
