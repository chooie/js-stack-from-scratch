{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "compat",
    "flowtype"
  ],
  "rules": {
    "compat/compat": "error",
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "globals": {
    "React": true
  },
  "settings": {
    "polyfills": ["fetch"]
  }
}
